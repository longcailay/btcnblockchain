@{
    ViewBag.Title = "Trang chủ";
}

<div class="row">
    @foreach (var list in ViewBag.AllChainContent)
    {
        <div class="col-md-4" style="border:groove;height:350px;width:auto">
            <h2>@list[0]</h2>
            <hr style="border:dashed">
            @foreach (var item in list)
            {
                <p>@item</p>
            }
        </div>
    }
</div>